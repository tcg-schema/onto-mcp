@prefix tcg:    <https://www.tcg-schema.org/core#> .
@prefix schema: <https://schema.org/> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .

<https://www.tcg-schema.org/core>
  a owl:Ontology ;
  rdfs:label "TCG Schema â€” Core"@en ;
  rdfs:comment "Core vocabulary for card games / trading card games / collectible card games, aligned with schema.org. Includes cards vs printings, sets, decks, formats, legality, mechanics, counters, resources, metagame archetypes/matchups, and LLM-friendly descriptors."@en ;
  owl:imports <https://schema.org/> .

################
# CORE CLASSES #
################

tcg:CardGame a rdfs:Class ;
  rdfs:label "CardGame"@en ;
  rdfs:subClassOf schema:Game .

tcg:CardSet a rdfs:Class ;
  rdfs:label "CardSet"@en ;
  rdfs:subClassOf schema:CreativeWork .

tcg:ProductRelease a rdfs:Class ;
  rdfs:label "ProductRelease"@en ;
  rdfs:subClassOf schema:Product .

tcg:Card a rdfs:Class ;
  rdfs:label "Card"@en ;
  rdfs:comment "Abstract card identity (concept), independent from specific printings."@en ;
  rdfs:subClassOf schema:CreativeWork .

tcg:CardFace a rdfs:Class ;
  rdfs:label "CardFace"@en ;
  rdfs:comment "A face/part of a card (double-faced, split, modal, etc.)."@en ;
  rdfs:subClassOf schema:Intangible .

tcg:CardPrinting a rdfs:Class ;
  rdfs:label "CardPrinting"@en ;
  rdfs:comment "A specific printing/variant (set, number, language, finish, etc.)."@en ;
  rdfs:subClassOf schema:Product .

tcg:Deck a rdfs:Class ;
  rdfs:label "Deck"@en ;
  rdfs:subClassOf schema:CreativeWork .

tcg:DeckListEntry a rdfs:Class ;
  rdfs:label "DeckListEntry"@en ;
  rdfs:subClassOf schema:Intangible .

tcg:Format a rdfs:Class ;
  rdfs:label "Format"@en ;
  rdfs:subClassOf schema:DefinedTerm .

tcg:Legality a rdfs:Class ;
  rdfs:label "Legality"@en ;
  rdfs:subClassOf schema:Intangible .

tcg:Mechanic a rdfs:Class ;
  rdfs:label "Mechanic"@en ;
  rdfs:comment "Named mechanic (keyword ability/action, resource system concept, etc.)."@en ;
  rdfs:subClassOf schema:DefinedTerm .

tcg:Ability a rdfs:Class ;
  rdfs:label "Ability"@en ;
  rdfs:subClassOf schema:Intangible .

tcg:Effect a rdfs:Class ;
  rdfs:label "Effect"@en ;
  rdfs:subClassOf schema:Intangible .

tcg:CounterType a rdfs:Class ;
  rdfs:label "CounterType"@en ;
  rdfs:comment "Counters as game objects (e.g., +1/+1, poison, charge)."@en ;
  rdfs:subClassOf schema:DefinedTerm .

tcg:CounterEffect a rdfs:Class ;
  rdfs:label "CounterEffect"@en ;
  rdfs:subClassOf tcg:Effect .

tcg:TargetSelector a rdfs:Class ;
  rdfs:label "TargetSelector"@en ;
  rdfs:subClassOf schema:Intangible .

tcg:Condition a rdfs:Class ;
  rdfs:label "Condition"@en ;
  rdfs:subClassOf schema:Intangible .

tcg:Role a rdfs:Class ;
  rdfs:label "Role"@en ;
  rdfs:comment "Functional role of a card in deckbuilding (removal, finisher, engine, etc.)."@en ;
  rdfs:subClassOf schema:DefinedTerm .

#############################
# METAGAME / COUNTERPLAY    #
#############################

tcg:Archetype a rdfs:Class ;
  rdfs:label "Archetype"@en ;
  rdfs:comment "Metagame archetype/strategy cluster (often format-dependent)."@en ;
  rdfs:subClassOf schema:CreativeWork .

tcg:Matchup a rdfs:Class ;
  rdfs:label "Matchup"@en ;
  rdfs:comment "A vs B relationship in a format with advantage and evidence."@en ;
  rdfs:subClassOf schema:Intangible .

tcg:CounterClaim a rdfs:Class ;
  rdfs:label "CounterClaim"@en ;
  rdfs:comment "Evidence-bearing claim that something counters something (card/mechanic/archetype) in a format/time window."@en ;
  rdfs:subClassOf schema:Intangible .

tcg:EvidenceBundle a rdfs:Class ;
  rdfs:label "EvidenceBundle"@en ;
  rdfs:subClassOf schema:Intangible .

#############################
# RESOURCES / COSTS (ABSTRACT)
#############################

tcg:ResourceSystem a rdfs:Class ;
  rdfs:label "ResourceSystem"@en ;
  rdfs:comment "A game's resource system used to pay costs (mana, ink, energy, etc.)."@en ;
  rdfs:subClassOf schema:Intangible .

tcg:ResourceType a rdfs:Class ;
  rdfs:label "ResourceType"@en ;
  rdfs:comment "A resource type within a system (colors, inks, energies, elements, etc.)."@en ;
  rdfs:subClassOf schema:DefinedTerm .

tcg:ResourceCost a rdfs:Class ;
  rdfs:label "ResourceCost"@en ;
  rdfs:comment "A cost expressed in a resource system, structured and/or textual."@en ;
  rdfs:subClassOf schema:Intangible .

tcg:ResourceCostComponent a rdfs:Class ;
  rdfs:label "ResourceCostComponent"@en ;
  rdfs:comment "One component of a cost: amount + optional resource type."@en ;
  rdfs:subClassOf schema:Intangible .

########################################
# LLM-FRIENDLY DESCRIPTORS (OPTIONAL)  #
########################################

tcg:DescriptorType a rdfs:Class ;
  rdfs:label "DescriptorType"@en ;
  rdfs:comment "Descriptor category (role, interaction, playPattern, synergyTag, etc.)."@en ;
  rdfs:subClassOf schema:DefinedTerm .

tcg:CardDescriptor a rdfs:Class ;
  rdfs:label "CardDescriptor"@en ;
  rdfs:comment "Descriptor assignment for a card with provenance and confidence."@en ;
  rdfs:subClassOf schema:Intangible .

tcg:ExtractionMethod a rdfs:Class ;
  rdfs:label "ExtractionMethod"@en ;
  rdfs:comment "How a descriptor was produced (regex, LLM, human, consensus, model-vX...)."@en ;
  rdfs:subClassOf schema:DefinedTerm .

tcg:DescriptorEvidence a rdfs:Class ;
  rdfs:label "DescriptorEvidence"@en ;
  rdfs:comment "Evidence supporting a descriptor (snippets, URLs, dataset ids, etc.)."@en ;
  rdfs:subClassOf schema:CreativeWork .

################
# PROPERTIES   #
################

# Common linking
